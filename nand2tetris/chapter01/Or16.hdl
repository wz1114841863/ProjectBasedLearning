/**
 * 16-bit Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Or = Not(Not(a) And Not(b));
    Not16(in=a[0..15], out=tmp1);
    Not16(in=b[0..15], out=tmp2);
    And16(a=tmp1, b=tmp2, out=tmp3);
    Not16(in=tmp3, out=out[0..15]);
}